Create Matrix myMatrix
Set myMatrix.shape <- (6, 9)

Loop myMatrix {
    if (Row == Col) {
        This <- 1 + 1
    } else {
        if (Row >= Col) {
            This <- 2
        } else {
            Create Fraction myFraction
            Set myFraction.numerator <- 1
            Set myFraction.denominator <- 2
            This <- myFraction
        }
    }
 }
End

Render myMatrix


